    {
      "cell_type": "code",
      "source": [
        "import PyPDF2\n",
        "\n",
        "# Path to the PDF file within the cloned repository\n",
        "pdf_file_path = '/content/AMCC_data/AMM_Report-2016_Digital.pdf'\n",
        "#https://github.com/nimrausa/AMCC_data/blob/main/AMM_Report-2016_Digital.pdf\n",
        "# Open the PDF file in read-binary mode\n",
        "with open(pdf_file_path, 'rb') as file:\n",
        "    # Create a PDF reader object\n",
        "    pdf_reader = PyPDF2.PdfReader(file)\n",
        "\n",
        "    # Initialize an empty string to store the extracted text\n",
        "    raw_text = \"\"\n",
        "\n",
        "    # Iterate over each page in the PDF\n",
        "    for page in pdf_reader.pages:\n",
        "        # Extract the text from the page and append it to the raw_text variable\n",
        "        raw_text += page.extract_text()\n",
        "\n",
        "# Print the extracted text\n",
        "print(raw_text)"
      ],
      "metadata": {
        "id": "0U5aHXg4LbRh"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "text_splitter = CharacterTextSplitter(\n",
        "    separator = \"\\n\",\n",
        "    chunk_size = 1000,\n",
        "    chunk_overlap  = 200,\n",
        "    length_function = len,\n",
        ")\n",
        "texts = text_splitter.split_text(raw_text)"
      ],
      "metadata": {
        "id": "kX1v3dpjNFuR"
      },
      "execution_count": null,
      "outputs": []
    },
